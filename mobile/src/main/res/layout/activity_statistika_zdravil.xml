<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.fzv.glucowatch.Statistika_zdravil"
    android:id="@+id/rel"
    android:background="#ff00d4ff">

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="200dip"
        android:id="@+id/graph" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Povprečna vrednost dodanega inzulina:"
        android:id="@+id/textView15"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Povprečna vrednost dodanega inzulina                           (zadnjih 10 dni):"
        android:id="@+id/textView17"
        android:layout_marginTop="26dp"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textViewAvgzdravil"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Minimalni vnos inzulina:"
        android:id="@+id/textView18"
        android:layout_marginTop="22dp"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textViewAvg10zdravil"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maximalni vnos inzulina:"
        android:id="@+id/textView19"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textViewminzdravil"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:layout_marginTop="22dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewAvgzdravil"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textView15"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewAvg10zdravil"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewminzdravil"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textView18"
        android:layout_alignLeft="@+id/textViewAvg10zdravil"
        android:layout_alignStart="@+id/textViewAvg10zdravil"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewmaxzdravil"
        android:textColor="#fffffefa"
        android:textSize="13dp"
        android:gravity="center"
        android:layout_below="@+id/textView19"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />
</RelativeLayout>
